{
  "name": "selling-lint-cli",
  "version": "0.0.4",
  "description": "s-lint命令行工具",
  "main": "./build",
  "scripts": {
    "test": "test",
    "scan-eslint-type": "node ./scripts/scan-eslint-type.js",
    "scan-deps": "node ./scripts/scan-deps.js",
    "build": "npm install && rimraf ./build && npm run scan-eslint-type && npm run scan-deps && tsc --build ./tsconfig.json"
  },
  "bin": {
    "s-lint": "./bin.js"
  },
  "files": [
    "build",
    "templates",
    "LICENSE.txt",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "s-lint",
    "command"
  ],
  "author": "zxy25",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "compressing": "^1.5.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.4",
    "handlebars": "^4.7.7",
    "inquirer": "^8.1.5",
    "minimatch": "^3.0.4",
    "ora": "^5.4.1",
    "pacote": "^12.0.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "gitHead": "7805f9086587487b0d882d89356650267c7386e5"
}
